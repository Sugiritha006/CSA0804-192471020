my_dict = {'a': 10, 'b': 25, 'c': 5, 'd': 40}

values = my_dict.values()
min_val = max_val = None

for v in values:
    if min_val is None or v < min_val:
        min_val = v
    if max_val is None or v > max_val:
        max_val = v

print("Minimum value:", min_val)
print("Maximum value:", max_val
